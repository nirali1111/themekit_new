                            <img src="{{ block.settings.scroll_image | img_url:'master' }}">
                        </div>
                        {% endif %}
             {% endif %}  
            <div class="page-width">
                
              <div class="sustainability-head">
                 {% if block.settings.logo_image != blank %}
                  <img src="{{ block.settings.logo_image  | img_url:"master"}}" alt="{{block.settings.logo_image.alt}}">
                {% endif %}
                 {% if block.settings.title != blank %}
                 <h2>{{block.settings.title}}</h2>
                {% endif %}
              </div>


              <div class="grid">
                 {% for i in (1..3) %}

                      {% capture image %}image_{{ i }}{% endcapture %}
                      {% capture title %}title_{{ i }}{% endcapture %}
                      {% capture content %}content_{{ i }}{% endcapture %}
                      {% assign image = block.settings[image] %}

                      {% if block.settings[image] != blank or block.settings[title] != blank or block.settings[content] != blank %}
                        <div class="grid__item medium-up--one-third">
                          <div class="sustainability-block">
                              <img src="{{ image | img_url:'master'}}">
                              {% if block.settings[title] != blank %}
                              <p><strong>{{ block.settings[title] }}</strong></p>
                              {% endif %}
                              {{ block.settings[content] }}
                          </div>
                        </div>
                        {% endif %}

                  {%endfor%} 
              </div>

            </div>
            {% if block.settings.section_bg_image != blank %}
            <div class="tab-img">
              <img src="{{ block.settings.section_bg_image  | img_url:"master"}}" alt="{{block.settings.section_bg_image.alt}}">
            </div>
            {% endif %}
          </div>
      </div>
	<!-- ========================== Sustainability End =================== -->

  {% endcase %}
{% endfor %}


