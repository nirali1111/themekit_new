{% layout 'password' %}

{% comment %}
  The contents of the password.liquid templates can be found in /sections
{% endcomment %}

{% section 'password-header' %}
{% section 'password-content' %}
{% section 'password-footer' %}

{% comment %}

{% for block in section.blocks %}
  {% case block.type %}

    {% when 'text-with-heading' %}
        <!-- ========================== section-2 ========================= -->
        {% if block.settings.title != blank and block.settings.text_content != blank %} 
              <section id="text-with-heading" class="learnmore-section section-padding {% if block.settings.scroll_image != blank %}top-img{%endif%} {% if block.settings.link != blank and  block.settings.button_text != blank %}buynow-btn{%endif%}" style="background-color:{{block.settings.section_bg_color}};">
                <div class="page-width">
                  <div class="{{block.settings.content_width}} {{block.settings.content_align}}">
            <!--       narrow   wide font-left font-right -->
                  {% if block.settings.link != blank and  block.settings.button_text != blank %}
                    <a href="{{block.settings.link}}" class="btn slideshow__btn paint-button">
                      <img src=" //cdn.shopify.com/s/files/1/0469/8905/0019/files/button-brush-orange_1.png?v=16813802238642895239">
                      <img src="//cdn.shopify.com/s/files/1/0469/8905/0019/files/button-brush-green.png?v=12416523813758496433" class="img-top">
                      <span class="paint-button-text">{{block.settings.button_text}}</span>
                    </a>
                    {%else%}
                        {% if block.settings.scroll_image != blank %}
                        <div class="leartop-img">
                            <img src="{{ block.settings.scroll_image | img_url:'master' }}">
                        </div>
                        {% endif %}
                    {% endif %}
                  
                  <div class="learmore-block">
                    <h2 class="p-title" {% if block.settings.heading_color != blank %}style="color: {{block.settings.heading_color}};" {% endif %}>{{block.settings.title}}</h2>
                    <div style="color:{{block.settings.content_color}};">
                    <p {% if block.settings.content_color != blank %} style="color: {{block.settings.content_color}};" {%endif%}> {{block.settings.text_content}} </p>
                  </div>
                  </div>
                </div>
                </div>
              </section>
            {%endif%}
        <!-- ========================== section-2-end ========================= -->
	{% when 'text-columns-with-image' %}
        <!-- ========================== section-3 ========================= -->
        {% if block.settings.title != blank and block.settings.text_content_left != blank and block.settings.text_content_right != blank %} 
            <section id="text-columns-with-image" class="natural-decline-section section-padding {% if block.settings.scroll_image != blank %}top-img{%endif%} {% if block.settings.link != blank and  block.settings.button_text != blank %}buynow-btn{%endif%}" style="background-color: {{block.settings.section_bg_color}};">
              <div class="page-width">
                <div class="{{block.settings.content_width}}">
                  {% if block.settings.link != blank and  block.settings.button_text != blank %}
                  <a href="{{block.settings.link}}" class="btn slideshow__btn paint-button">
                      <img src=" //cdn.shopify.com/s/files/1/0469/8905/0019/files/button-brush-orange_1.png?v=16813802238642895239">
                      <img src="//cdn.shopify.com/s/files/1/0469/8905/0019/files/button-brush-green.png?v=12416523813758496433" class="img-top">
                      <span class="paint-button-text">{{block.settings.button_text}}</span>
                    </a>
                  {%else%}
                        {% if block.settings.scroll_image != blank %}
                        <div class="leartop-img">
                            <img src="{{ block.settings.scroll_image | img_url:'master' }}">
                        </div>
                        {% endif %}
                  {% endif %}
                  
                  {% if block.settings.section_image != blank %}
                    <div class="natural-block">
                      <img src="{{ block.settings.section_image | img_url : "master" }}" alt="">
                    </div>
                  {% endif %}

                 <div class="natural-block-content">
                   <h3 {% if block.settings.heading_color != blank %}style="color: {{block.settings.heading_color}};" {% endif %}>
                     <strong>{{block.settings.title}}</strong>
                   </h3>

                     <div class="natural-block-left">
                       {{ block.settings.text_content_left }}
                     </div>

                     <div class="natural-block-right" >
                       {{ block.settings.text_content_right }}
                     </div>

                  </div>
                </div>
              </div>
            </section>
            <style>
              .natural-block-left * , .natural-block-right * { color: {{ block.settings.content_color }}}
            </style>
        {%endif%}
        <!-- ========================== section-3-end ========================= -->
	{% when 'image-with-text-overlay' %}
		<!-- ========================== section-4-start ========================= -->
		{% if block.settings.title != blank and block.settings.text_content != blank %} 
			<section id="image-with-text-overlay" class="image-with-text-overlay section-padding {% if block.settings.scroll_image != blank %}top-img{%endif%} {% if block.settings.link != blank and  block.settings.button_text != blank %}buynow-btn{%endif%}" {% if block.settings.section_bg_image != blank %}style="background-image:url('{{block.settings.section_bg_image | img_url:"master"}}')"{% endif %}>
           
          {% if block.settings.link != blank and  block.settings.button_text != blank %}
              <a href="{{block.settings.link}}" class="btn slideshow__btn paint-button">
                <img src=" //cdn.shopify.com/s/files/1/0469/8905/0019/files/button-brush-orange_1.png?v=16813802238642895239">
                <img src="//cdn.shopify.com/s/files/1/0469/8905/0019/files/button-brush-green.png?v=12416523813758496433" class="img-top">
                <span class="paint-button-text">{{block.settings.button_text}}</span>
              </a>
              {%else%}
                 {% if block.settings.scroll_image != blank %}
                  <div class="leartop-img">
                      <img src="{{ block.settings.scroll_image | img_url:'master' }}">
                  </div>
                 {% endif %}
          {%endif%}
          
        <div class="image-with-overlay-content" style="background: {{ block.settings.content_bg_color }};">
           <h2 class="p-title" style="{%if block.settings.heading_color != blank %}color: {{block.settings.heading_color}};{%endif%} {% if block.settings.heading_align %}text-align:{{ block.settings.heading_align }};{% endif %}">{{block.settings.title}}</h2>
           <h3 style="{% if block.settings.sub_heading_align %} text-align:{{block.settings.sub_heading_align}};{% endif %} {% if block.settings.sub_heading_color != blank %}color:{{block.settings.sub_heading_color}}; {% endif %}">
            <strong>{{block.settings.subtitle}}</strong></h3>
            <ul>
              {{block.settings.text_content}}
            </ul>
           {% if block.settings.inner_button_link != blank and block.settings.inner_button_text != blank %}
           <div class="image-with-text-overlay-btn">
                <a href="{{block.settings.inner_button_link}}" class="btn white-btn">{{block.settings.inner_button_text}}</a>
           </div>
           {% endif %}
         </div>
        </section>
		{%endif%}
		<!-- ========================== section-4-end ========================= -->
	{% when 'video-section' %}
		<!-- ========================== section-5-start ========================= -->
        <section id="video-section" class="video-section section-padding  {% if block.settings.scroll_image != blank %}top-img{%endif%} {% if block.settings.link != blank and  block.settings.button_text != blank %}buynow-btn{%endif%}" {% if block.settings.section_bg_color != blank %} style="background-color: {{ block.settings.section_bg_color }};" {% endif %}>
          <div class="page-width">
             {% if block.settings.video_1_url != blank %} 
                <div class="video-block">
                  {% if block.settings.title != blank %} 
                  	<h3 class="{{ block.settings.size}}" style="color:{{ block.settings.color }}"><strong>{{ block.settings.title }}</strong></h3>
                  {% endif %}
                  
                 <!-- add class large-quote  -->
                  {% if block.settings.subtitle1 != blank %} 
                    <p style="color:{{ block.settings.subcolor }}">{{ block.settings.subtitle1 }}</p>
                  {% endif %}
                  
                  <div class="iframe-wrapper">
                  <iframe src="https://www.youtube.com/embed/{{ block.settings.video_1_url.id }}?start=180&amp;wmode=opaque&amp;enablejsapi=1" scrolling="no" allowfullscreen="" width="854" height="480" frameborder="0" id="yui_3_17_2_1_1602161379266_104"><br/></iframe>
                  </div>
                  
                  {% if block.settings.description != blank %} 
                    <div class="video-dec">
                   {{ block.settings.description }}
                    </div>
                  {% endif %}
                </div>
            <style>
              .video-dec * { color: {{ block.settings.descriptioncolor }} };
            </style>
            {% endif %}
            
            {% if block.settings.video_2_url != blank %} 
            <div class="video-block">
              
              {% if block.settings.subtitle2 != blank  %}
              	<h3 class="{{ block.settings.size1}}" style="color:{{ block.settings.color }}"><strong>{{ block.settings.subtitle2 }}</strong></h3>
              {% endif %}
              
              <div class="iframe-wrapper">
              <iframe src="https://www.youtube.com/embed/{{ block.settings.video_2_url.id }}?wmode=opaque&amp;enablejsapi=1" scrolling="no" allowfullscreen="" width="854" height="480" frameborder="0" id="yui_3_17_2_1_1602161379266_146"><br/></iframe>
              </div>
                {% if block.settings.description1 != blank %} 
                <div class="video-dec">
                   {{ block.settings.description1 }}
                   </div>
                  {% endif %}
              </div>
            <style>
              .video-dec * { color: {{ block.settings.descriptioncolor1 }} };
            </style>
            {% endif %}
          </div>
        </section>
		<!-- ========================== section-5-end ========================= -->
	{% when 'three-steps-section' %}
        <!-- ========================== section-6-start =================== -->
        <section id="three-steps-section" class="icon-with-text-section {% if block.settings.section_border_img != blank %}border-img{% endif %} section-padding {% if block.settings.scroll_image != blank %}top-img{%endif%} {% if block.settings.link != blank and  block.settings.button_text != blank %}buynow-btn{%endif%}" style="{% if block.settings.section_bg_color != blank %}background-color: {{block.settings.section_bg_color}};{%endif%} {% if block.settings.section_border_img != blank %} border-image-source:url('{{block.settings.section_border_img | img_url:"master"}}');{%endif%}">
          <!-- border-img optional -->
          <div class="page-width">
            {% if block.settings.link != blank and block.settings.button_text != blank %}
              <a href="{{block.settings.link}}" class="btn slideshow__btn paint-button">
                <img src=" //cdn.shopify.com/s/files/1/0469/8905/0019/files/button-brush-orange_1.png?v=16813802238642895239">
                <img src="//cdn.shopify.com/s/files/1/0469/8905/0019/files/button-brush-green.png?v=12416523813758496433" class="img-top">
                <span class="paint-button-text">{{block.settings.button_text}}</span>
              </a>
            {%else%}
              {% if block.settings.scroll_image != blank %}
                <div class="leartop-img">
                    <img src="{{ block.settings.scroll_image | img_url:'master' }}">
                </div>
              {% endif %}
            {% endif %}
             
           <div class="wide">
             {% if block.settings.scroll_image != blank %}
                <div class="leartop-img">
                    <img src="{{ block.settings.scroll_image | img_url:'master' }}">
                </div>
              {% endif %}
           
            <div class="icon-with-text-content">
              {% if block.settings.title != blank %}
              <h3 {% if block.settings.heading_color != blank %}style="color: {{block.settings.heading_color}};" {%endif%}><strong>{{block.settings.title}}</strong></h3>
				{% endif %}
              
              {% if block.settings.img1 != blank %}
              <div class="icon-with-text-block">
                {% if block.settings.img1 != blank%}
                <div class="icon-with-text-block-img">
                  <img class="{{ block.settings.image_change }}" src="{{ block.settings.img1 | img_url:'master' }}" alt="">
                </div>
                {% endif %}
                <div class="icon-with-text-block-content">
                {% if block.settings.Step_1_title != blank%}
                  <p class="title" {% if block.settings.Step_1_title_color != blank %}style="color:{{block.settings.Step_1_title_color}};"{%endif%}><strong>{{block.settings.Step_1_title}}</strong></p>
                {% endif %}
                <p {% if block.settings.Step_1_title_color != blank %}style="color:{{block.settings.Step_1_title_color}};"{%endif%} class="line-separator">–––</p>
                <div class="icon-content">{{block.settings.step_1_content}}</div>
                </div>
              </div>
              {%endif%}

              {% if block.settings.img2 != blank %}
              <div class="icon-with-text-block">
                 {% if block.settings.img2 != blank %}
                 <div class="icon-with-text-block-img">
                  <img class="{{ block.settings.image_change }}" src="{{ block.settings.img2 | img_url:'master' }}" alt="">
                </div>
                {% endif %}
                <div class="icon-with-text-block-content">
                {% if block.settings.Step_2_title != blank %}
                  <p class="title" {% if block.settings.Step_2_title_color != blank %}style="color:{{block.settings.Step_2_title_color}};"{%endif%}><strong>{{block.settings.Step_2_title}}</strong></p>
                {% endif %}
                <p {% if block.settings.Step_2_title_color != blank %}style="color:{{block.settings.Step_2_title_color}};"{%endif%} class="line-separator">–––</p>
                <div class="icon-content">{{block.settings.step_2_content}}</div>
                </div>
              </div>
              {%endif%}
              
			 {% if block.settings.img3 != blank %}
              <div class="icon-with-text-block">
                 {% if block.settings.img3 != blank %}
                 <div class="icon-with-text-block-img">
                  <img class="{{ block.settings.image_change }}" src="{{ block.settings.img3 | img_url:'master' }}" alt="">
                </div>
                {% endif %}
                <div class="icon-with-text-block-content">
                {% if block.settings.Step_3_title != blank %}
                  <p class="title" {% if block.settings.Step_3_title_color != blank %}style="color:{{block.settings.Step_3_title_color}};"{%endif%}><strong>{{block.settings.Step_3_title}}</strong></p>
                {% endif %}
                <p {% if block.settings.Step_3_title_color != blank %}style="color:{{block.settings.Step_3_title_color}};"{%endif%} class="line-separator">–––</p>
                <div class="icon-content">{{block.settings.step_3_content}}</div>
                </div>
              </div>
			{%endif%}
              
            </div>
          </div>
          </div>
        </section>
        <style>
          .icon-content * , .icon-content {color:{{ block.settings.contentcolor }};}
        </style>
        <!-- ========================== section-6-end =================== -->
	{% when 'why-buy-section' %}
		<!-- ========================== section-7-start =================== -->
        <section id="why-buy-section" class="why-buy-section section-padding {% if block.settings.scroll_image != blank %}top-img{%endif%} {% if block.settings.link != blank and  block.settings.button_text != blank %}buynow-btn{%endif%}">
          <div class="page-width">
            {% if block.settings.link != blank and block.settings.button_text != blank %}
              <a href="{{block.settings.link}}" class="btn slideshow__btn paint-button">
                <img src=" //cdn.shopify.com/s/files/1/0469/8905/0019/files/button-brush-orange_1.png?v=16813802238642895239">
                <img src="//cdn.shopify.com/s/files/1/0469/8905/0019/files/button-brush-green.png?v=12416523813758496433" class="img-top">
                <span class="paint-button-text">{{block.settings.button_text}}</span>
              </a>
            {%else%}
                {% if block.settings.scroll_image != blank %}
                <div class="leartop-img">
                    <img src="{{ block.settings.scroll_image | img_url:'master' }}">
                </div>
              {% endif %}
            {%endif%}
			
            {% if block.settings.logo != blank %}
            <div class="whyus-logo">
              <img src="{{ block.settings.logo | img_url:"master"}}" alt="">
            </div>
            {% endif %}
            
            {% if block.settings.title != blank %}
            <h2 class="font-center" {% if block.settings.heading_color != blank %}style="color:{{block.settings.heading_color}};"{%endif%}>{{block.settings.title}}</h2>
            {% endif %}
            
            {%if block.settings.left_content != blank and block.settings.right_content != blank %}
            <div class="whyus-content">
              <div class="whyus-block">
                <p>{{ block.settings.left_content }}</p>      
              </div>
              <div class="whyus-block">
                <p>{{ block.settings.right_content }}</p>      
              </div>
            </div>
            {% endif %}
            <div class="whyus-content-more">
			{% for i in (1..6) %}
             {% capture doc_link %}block_{{ i }}_doc_link{% endcapture %}
             {% capture doc_link_text %}block_{{ i }}_doc_link_text{% endcapture %}
             {% capture content %}block_{{ i }}_content{% endcapture %}
             {% capture labLogo %}block_{{ i }}_lab_logo{% endcapture %}

            {% if block.settings[content]  != blank %}
            <div class="whyus-mblock">
                {% if block.settings[doc_link]  != blank %}
                <a class="iframe-popup"  href="{{block.settings[doc_link]}}" target="_blank" >
                {% endif %}
                    <div class="whyus-mblock-content">
                      <p>{{block.settings[content]}}</p>
                      {% if block.settings[doc_link_text]  != blank %} <p class="link">{{block.settings[doc_link_text]}}</p> {% endif %}

                        {% if block.settings[labLogo]  != blank %}
                        {% assign lab_log_img = block.settings[labLogo] %}
                            <div class="lab-logo">
                              <img src="{{ lab_log_img | img_url: 'master' }}">
                            </div>
                        {% endif %}
                    </div>
                 </a>
              </div>
              {% endif %}
            {%endfor%}
            </div>
          </div>
        </section>
        <!-- ========================== section-7-end =================== -->
	{% when 'product-guide' %}
		<!-- ========================== section-8-start =================== -->
         <section id="product-guide" class="product-guide section-padding" {% if block.settings.section_bg_img != blank %}style="background-image:url('{{ block.settings.section_bg_img | img_url:"master" }}')"{%endif%}>
             <div class="page-width">
               <div class="wide">
                 <div class="product-guide-row">
                   {% if block.settings.product_img != blank %}
                   <div class="p-col col3">
                     <img src="{{ block.settings.product_img | img_url:"master" }}" alt="">
                   </div>
                   {% endif %}
                   
                    {% for i in (1..3) %}

                     {% capture icon %}feature_{{ i }}_img{% endcapture %}
                     {% capture text %}feature_{{ i }}_text{% endcapture %}

                    {% if block.settings[text]  != blank %}
                        <div class="p-col col2">
                          <div class="product-guide-content">
                            {% if block.settings[icon]  != blank %}
                                {% assign feature_img = block.settings[icon] %}
                                <img src="{{ feature_img | img_url: 'master' }}">
                           {%endif%}
                           <p><strong>{{ block.settings[text] }}</strong></p>
                         </div>
                        </div>
                      {% endif %}
                    {%endfor%}
                   
                   {% if block.settings.link != blank and block.settings.button_text != blank %}
                   <div class="p-col col3">
                     <div class="font-center">
                     <a href="{{block.settings.link}}" class="btn small-btn">{{block.settings.button_text}}</a>
                     </div>
                   </div>
                   {% endif %}
                   
               </div>
             </div>
           </div>
         </section>
        <!-- ========================== section-8-end =================== -->
        <!-- ========================== section-9-start =================== -->

        <!-- ========================== section-9-end =================== -->
	{% when 'tab-section' %}
        <!-- ========================== section-10-start =================== -->
          <section id="tab-section" class="tab-section section-padding {% if block.settings.scroll_image != blank %}top-img{%endif%} {% if block.settings.link != blank and  block.settings.button_text != blank %}buynow-btn{%endif%}" {% if block.settings.section_bg_img != blank %}style="background-image:url('{{ block.settings.section_bg_img | img_url:"master"}}')"{%endif%}>
            <div class="page-width">
              {% if block.settings.link != blank and  block.settings.button_text != blank %}
              <a href="{{block.settings.link}}" class="btn slideshow__btn paint-button">
                <img src=" //cdn.shopify.com/s/files/1/0469/8905/0019/files/button-brush-orange_1.png?v=16813802238642895239">
                <img src="//cdn.shopify.com/s/files/1/0469/8905/0019/files/button-brush-green.png?v=12416523813758496433" class="img-top">
                <span class="paint-button-text">{{ block.settings.button_text }}</span>
              </a>
              {%else%}
                {% if block.settings.scroll_image != blank %}
                <div class="leartop-img">
                    <img src="{{ block.settings.scroll_image | img_url:'master' }}">
                </div>
              {% endif %}
              {% endif %}
               
              <div class="tab-block" {% if block.settings.tab_Block_bg_color != blank %}style="background: {{block.settings.tab_Block_bg_color}};"{%endif%}>
                {% if block.settings.heading != blank %}<h2 class="font-center p-title">{{ block.settings.heading }}</h2>{%endif%}
                <div class="tab-block-content">
                    <div class="tabs">
                      <ul id="tabs-nav">
                        {% for i in (1..4) %}
                          {% capture title %}tab_{{ i }}_title{% endcapture %}
                           {% capture content %}tab_{{ i }}_content{% endcapture %}
                          {% if block.settings[title]  != blank and block.settings[content]  != blank %}
                              <li><a href="#tab{{ i }}">{{block.settings[title]}}</a></li>
                            {% endif %}
                          {%endfor%}
                      </ul> <!-- END tabs-nav -->
                      <div id="tabs-content">
                        {% for i in (1..4) %}
                          {% capture title %}tab_{{ i }}_title{% endcapture %}
                          {% capture content %}tab_{{ i }}_content{% endcapture %}

                          {% if block.settings[title]  != blank and block.settings[content]  != blank %}
                                <div id="tab{{ i }}" class="tab-content">
                                    {{block.settings[content]}}
                                </div>
                            {% endif %}
                        {%endfor%}

                      </div> <!-- END tabs-content -->
                    </div> <!-- END tabs -->
                </div>
              </div>
            </div>
          </section>
        <!-- ========================== section-10-end =================== -->
	{% when 'faq-section' %}
        <!-- ========================== section-11-start =================== -->
        	<section id="faq-section"  class="faq-section section-padding {% if block.settings.scroll_image != blank %}top-img{%endif%} {% if block.settings.link != blank and  block.settings.button_text != blank %}buynow-btn{%endif%}" {% if block.settings.section_bg_color != blank %} style="background-color: {{block.settings.section_bg_color}};" {%endif%}>
              {% if block.settings.link != blank and block.settings.button_text != blank %}
                  <a href="{{ block.settings.link }}" class="btn slideshow__btn paint-button">
                    <img src=" {{ 'button-brush-orange_1.png'| file_url}}">
                    <img src="{{ 'button-brush-green.png'| file_url}}" class="img-top">
                    <span class="paint-button-text">{{ block.settings.button_text }}</span>
                  </a>
              {%else%}
                    {% if block.settings.scroll_image != blank %}
                <div class="leartop-img">
                    <img src="{{ block.settings.scroll_image | img_url:'master' }}">
                </div>
              {% endif %}
                {%endif%}

           <div class="page-width">
              {% if block.settings.heading %}<h2 style="text-align:center;white-space:pre-wrap;">{{block.settings.heading}}</h2>{%endif%}
              <div class="faq-content">
                {% for i in (1..8) %}
                    {% capture question %}que_{{ i }}{% endcapture %}
                    {% capture answer %}ans_{{ i }}{% endcapture %}
                    {% capture doc_1_link %}ans_{{ i }}_doc_1{% endcapture %}
                    {% capture doc_1_link_text %}ans_{{ i }}_doc_1_text{% endcapture %}
                    {% capture doc_2_link %}ans_{{ i }}_doc_2{% endcapture %}
                    {% capture doc_2_link_text %}ans_{{ i }}_doc_2_text{% endcapture %}
                    {% capture learnmore_link %}ans_{{ i }}_learn_more_link{% endcapture %}
                    {% capture learnmore_text %}ans_{{ i }}_learn_more_text{% endcapture %}
                    {% if block.settings[question]  != blank and block.settings[answer]  != blank %}
                         <div class="set">
                            <h4 href="#" class="">
                               {{block.settings[question]}}
                               <i class="fa fa-plus"></i>
                            </h4>
                            <div class="content">
                        <p>{{block.settings[answer]}}</p>
                        <!-- Document Links -->

                        {% if block.settings[doc_1_link]  != blank and block.settings[doc_1_link_text]  != blank %}
                        <u><a href="{{ block.settings[doc_1_link] }}" target="_blank" rel="nofollow"><strong>{{ block.settings[doc_1_link_text] }}</strong></a></u><br>
                        {% endif %}

                        {% if block.settings[doc_2_link]  != blank and block.settings[doc_2_link_text]  != blank %}
                        <u><a href="{{ block.settings[doc_2_link] }}" target="_blank" rel="nofollow"><strong>{{ block.settings[doc_2_link_text] }}</strong></a></u>
                        {% endif %}

                        <!-- Learn more Links -->
                        {% if block.settings[learnmore_link]  != blank and block.settings[learnmore_text]  != blank %}
